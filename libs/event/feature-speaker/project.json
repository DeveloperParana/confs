{
  "projectType": "library",
  "sourceRoot": "libs/event/feature-speaker/src",
  "prefix": "confs",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/event/feature-speaker"],
