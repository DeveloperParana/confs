  "scripts": {
    "nx": "nx",
    "start": "nx run-many --target=serve --projects=server,conf --parallel",
    "build": "nx build",
    "test": "nx test",
    "build:server": "npx nx build server --prod",
    "build:cert": "npx nx build cert --prod",
