{
  "name": "event-feature-page",
  "projectType": "library",
  "sourceRoot": "libs/event/feature-page/src",
  "prefix": "confs",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/event/feature-page"],
      "options": {
        "jestConfig": "libs/event/feature-page/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/event/feature-page/**/*.ts",
          "libs/event/feature-page/**/*.html"
        ]
      }
    }
  },
  "tags": ["type:feature", "scope:event"]
}
