{
  "projectType": "library",
  "sourceRoot": "libs/shared/project/feature-project/src",
  "prefix": "confs",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/project/feature-project"],
      "options": {
        "jestConfig": "libs/shared/project/feature-project/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/project/feature-project/**/*.ts",
          "libs/shared/project/feature-project/**/*.html"
        ]
      }
    }
  },
  "tags": ["type:feature", "scope:shared", "domain:project"]
}
