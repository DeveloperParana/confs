{
  "name": "conf",
  "projectType": "application",
  "sourceRoot": "apps/conf/src",
  "prefix": "conf",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/conf/browser",
        "index": "apps/conf/src/index.html",
        "main": "apps/conf/src/main.ts",
        "polyfills": "apps/conf/src/polyfills.ts",
        "tsConfig": "apps/conf/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/conf/src/CNAME",
          "apps/conf/src/sitemap.xml",
          "apps/conf/src/assets",
          {
            "input": "./assets/media-kit",
            "glob": "**/*.*",
            "output": "./media-kit"
          },
          {
            "input": "./assets/icons",
            "glob": "**/*.svg",
            "output": "./assets/icons"
          }
        ],
        "styles": ["apps/conf/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/conf/src/environments/environment.ts",
              "with": "apps/conf/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "baseHref": "/"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "conf:build:production"
        },
        "development": {
          "browserTarget": "conf:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/conf/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "conf:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/conf/**/*.ts", "apps/conf/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/conf"],
      "options": {
        "jestConfig": "apps/conf/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "server": {
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/apps/conf/server",
        "main": "apps/conf/server.ts",
        "tsConfig": "apps/conf/tsconfig.server.json",
        "inlineStyleLanguage": "scss"
      },
      "configurations": {
        "production": {
          "outputHashing": "media",
          "fileReplacements": [
            {
              "replace": "apps/conf/src/environments/environment.ts",
              "with": "apps/conf/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false
        }
      },
      "defaultConfiguration": "production",
      "production": {
        "outputHashing": "media",
        "fileReplacements": [
          {
            "replace": "apps/conf/src/environments/environment.ts",
            "with": "apps/conf/src/environments/environment.prod.ts"
          }
        ]
      },
      "development": {
        "optimization": false,
        "sourceMap": true,
        "extractLicenses": false
      }
    },
    "serve-ssr": {
      "executor": "@nguniversal/builders:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "conf:build:development",
          "serverTarget": "conf:server:development"
        },
        "production": {
          "browserTarget": "conf:build:production",
          "serverTarget": "conf:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "prerender": {
      "executor": "@nguniversal/builders:prerender",
      "options": {
        "routes": ["/"]
      },
      "configurations": {
        "production": {
          "browserTarget": "conf:build:production",
          "serverTarget": "conf:server:production"
        },
        "development": {
          "browserTarget": "conf:build:development",
          "serverTarget": "conf:server:development"
        }
      },
      "defaultConfiguration": "production"
    }
  },
  "tags": ["type:app"],
  "namedInputs": {
    "projectSpecificFiles": ["{workspaceRoot}/assets/media-kit/**/*.*"]
  }
}
